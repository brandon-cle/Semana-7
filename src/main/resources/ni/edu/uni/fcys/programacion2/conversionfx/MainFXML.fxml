            </VBox>
         </left>
         <center>
            <VBox fx:id="vbox_center" alignment="CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
   </children>
</AnchorPane>
