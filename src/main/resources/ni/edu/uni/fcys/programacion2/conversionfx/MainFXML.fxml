<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/15.0.1" fx:controller="ni.edu.uni.fcys.programacion2.conversionfx.controller.MainFXMLController">
    <stylesheets>
        <URL value="@/ni/edu/uni/fcys/programacion2/conversionfx/styles/temperaturefxml.css" />
    </stylesheets>
   <children>
      <BorderPane layoutX="175.0" layoutY="100.0" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <left>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="20.0" style="-fx-border-color: Gray;" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="btn_temperature" alignment="BOTTOM_CENTER" mnemonicParsing="false" onAction="#btn_temperatureAction" prefHeight="100.0" prefWidth="90.0" text="Temperature" />
                  <Button fx:id="btn_currency" alignment="BOTTOM_CENTER" mnemonicParsing="false" onAction="#btn_currencyAction" prefHeight="100.0" prefWidth="90.0" text="Currency" />
               </children>
               <padding>
                  <Insets bottom="20.0" top="20.0" />
               </padding>
               <BorderPane.margin>
                  <Insets bottom="0.2" top="0.2" />
